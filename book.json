{
    "author":"zhuyuqiong",
    "plugins": [
        "github",
        "prism",
        "-highlight",
        "anchors",
        "splitter",
        "anchor-navigation-ex",
        "theme-comscore",
        "-search",
        "search-pro",
        "-sharing",
        "sharing-plus",
        "editlink"
        ],
    "pluginsConfig": {
        "github": {
        "url": "https://github.com/zhuyuqiong/snadk-developer-guide"
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
        "anchor-navigation-ex":{
            "multipleH1":false,
            "showLevel":false,
            "mode":"float"
        },
        "editlink": {
          "base": "https://www.gitbook.com/book/zhuyuqiong/snadk-developer-guide/edit#/edit/master",
          "label": "Edit This Page",
          "multilingual": false
        },
        "sharing": {
            "douban": false,
            "facebook": true,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook", "douban", "twitter","weibo", "qq"
            ]
        }
    } 
}