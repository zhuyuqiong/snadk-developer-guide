{
    "root":"docs"
    "plugins": [
    "github",
    "prism",
    "-highlight",
    "splitter",
    "anchors",
    "book-summary-scroll-position-saver",
    "anchor-navigation",
    "theme-comscore",
    "-search",
    "search-pro",
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/zhuyuqiong/snadk-developer-guide"
        },
        "search-pro": {
        "cutWordLib": "nodejieba",
        "defineWord" : ["Gitbook Use"]
        },
        "sharing": {
        "weibo": true,
        "facebook": true,
        "twitter": true,
        "google": false,
        "instapaper": false,
        "vk": false,
        "all": [
            "facebook", "google", "twitter",
                "weibo", "instapaper"
        ]
        }
    }
}